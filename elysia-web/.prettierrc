{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "^@/hooks/(.*)$",
    "^@/lib/(.*)$",
    "^@/utils/(.*)$",
    "^@/schemas/(.*)$",
    "^@/queries/(.*)$",
    "^@/components/(.*)$",
    "^@/modules/(.*)$",
    "^@/(.*)$",
    "^[./]",
    "^[^./].*\\.css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
